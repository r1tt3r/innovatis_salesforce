<?xml version="1.0" encoding="UTF-8"?>
<preferences xmlns="http://www.demandware.com/xml/impex/preferences/2007-03-31">
    <standard-preferences>
        <all-instances>
            <preference preference-id="AddProductToBasketBehavior">IncrementQuantities</preference>
            <preference preference-id="CouponGeneratorIncludeDashes">true</preference>
            <preference preference-id="CouponGeneratorIncludeVowels">true</preference>
            <preference preference-id="EnableSiteOrderNumbers">false</preference>
            <preference preference-id="OrderEditExportDelay">30</preference>
            <preference preference-id="OrderExportDelay">30</preference>
            <preference preference-id="OrderIPLoggingEnabled">true</preference>
            <preference preference-id="OrderLifetime">0</preference>
            <preference preference-id="AccountNumberRetentionDays">0</preference>
            <preference preference-id="SiteCatalog">organicfood-catalog</preference>
            <preference preference-id="SiteCurrencies">BRL</preference>
	        <preference preference-id="SiteCustomerList"/>
            <preference preference-id="SiteDefaultLocale">pt_BR</preference>
            <preference preference-id="SiteLibrary">OrganicFoodSharedLibrary</preference>
            <preference preference-id="SiteInventoryList">inventory_m_organicfood</preference>
            <preference preference-id="SiteLocales">default:pt_BR</preference>
            <preference preference-id="SitePriceBooks">brl-organicfood-list-prices</preference>
            <preference preference-id="StorefrontURLsEnabled">true</preference>
        </all-instances>
        <development>
            <preference preference-id="AvailabilityFiltering">Disabled</preference>
            <preference preference-id="AvailabilityLowRankingThreshold">0.0</preference>
            <preference preference-id="IncludePromotionIDInProductIndex">true</preference>
            <preference preference-id="PageCacheEnabled">true</preference>
            <preference preference-id="RobotsTxtContent"/>
            <preference preference-id="RobotsTxtMode">Cartridge</preference>
            <preference preference-id="SearchSuggestions">Disabled</preference>
            <preference preference-id="StaticCacheTTL">2592000</preference>
        </development>
        <staging>
            <preference preference-id="AvailabilityFiltering">Disabled</preference>
            <preference preference-id="AvailabilityLowRankingThreshold">0.0</preference>
            <preference preference-id="IncludePromotionIDInProductIndex">true</preference>
            <preference preference-id="PageCacheEnabled">false</preference>
            <preference preference-id="RobotsTxtContent"/>
            <preference preference-id="RobotsTxtMode">Cartridge</preference>
            <preference preference-id="SearchSuggestions">Enabled</preference>
            <preference preference-id="StaticCacheTTL">0</preference>
        </staging>
        <production>
            <preference preference-id="AvailabilityFiltering">Disabled</preference>
            <preference preference-id="AvailabilityLowRankingThreshold">0.0</preference>
            <preference preference-id="IncludePromotionIDInProductIndex">true</preference>
            <preference preference-id="PageCacheEnabled">true</preference>
            <preference preference-id="RobotsTxtContent"/>
            <preference preference-id="RobotsTxtMode">Cartridge</preference>
            <preference preference-id="SearchSuggestions">Disabled</preference>
            <preference preference-id="StaticCacheTTL">2592000</preference>
        </production>
    </standard-preferences>
    <custom-preferences>
        <all-instances/>
        <development/>
        <staging/>
        <production/>
    </custom-preferences>
</preferences>

